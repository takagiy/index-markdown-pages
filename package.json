{
  "name": "index-markdown-pages",
  "version": "0.1.0",
  "description": "",
  "exports": {
    ".": {
      "require": {
        "default": "./dist/index.js"
      },
      "default": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "swc src --out-dir dist --strip-leading-paths --config module.type=commonjs && echo '{ \"type\": \"commonjs\" }' > dist/package.json",
    "lint": "biome check; status=$?; echo 'Use `pnpm run lint!` to apply the fixes.'; exit $status",
    "lint!": "biome check --write",
    "test": "pnpm run test:types && ava",
    "test:types": "tsc --noEmit",
    "version": "pnpm run lint!",
    "prepublishOnly": "pnpm run lint",
    "prepare": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/takagiy/index-marldown-pages.git"
  },
  "keywords": [],
  "author": "Yuki Takagi <takagiy.4dev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/takagiy/index-marldown-pages/issues"
  },
  "homepage": "https://github.com/takagiy/index-marldown-pages/#readme",
  "packageManager": "pnpm@9.4.0+sha256.b6fd0bfda555e7e584ad7e56b30c68b01d5a04f9ee93989f4b93ca8473c49c74",
  "ava": {
    "files": [
      "test/**/*.ts"
    ],
    "extensions": [
      "ts"
    ],
    "nodeArguments": [
      "-r",
      "@swc-node/register"
    ],
    "environmentVariables": {
      "SWCRC": "true"
    }
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@swc-node/register": "^1.10.0",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.6.6",
    "@tsconfig/node-lts": "^20.1.3",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^20.14.9",
    "ava": "^6.1.3",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.0",
    "globby": "^14.1.0"
  }
}
